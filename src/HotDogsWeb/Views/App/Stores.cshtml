@model IEnumerable<HotDogsWeb.Models.HotDogStore>
@{
    ViewBag.Title = "Stores Page";
    Layout = "_layout";
}
<div class="row">
    <div class="col-md-6">
        <h1>Rays' HotDogs</h1>
        <p>This will be a fun website with HotDogs !</p>

        <form>
            <div class="form-group">
                <label>Date</label>
                <input class="form-control" />
            </div>
            <div class="form-group">
                <label>Location</label>
                <input class="form-control" />
            </div>
            <input type="submit" value="Add" class="btn btn-success" />
        </form>
    </div>

    <div class="col-md-6">
        <h2>My HotDogs Stores</h2>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Name : @item.Location</li>
            }
        </ul>
    </div>

</div>